generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id          Int      @id @default(autoincrement())
  temperature Float
  lightLevel  Int
  isAlarm     Boolean
  timestamp   DateTime @default(now())

  @@map("sensor_data")
}